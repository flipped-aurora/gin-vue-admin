package initialize

import (
	"context"
	"fmt"
	model "github.com/flipped-aurora/gin-vue-admin/server/model/system"
	"github.com/flipped-aurora/gin-vue-admin/server/plugin/utils"
	"go.uber.org/zap"
)

func Menu(ctx context.Context) {
    entities := []model.SysBaseMenu{}
	err := utils.Register.Menus(ctx, entities)
	if err != nil {
		zap.L().Error(fmt.Sprintf("%+v", err))
	}
}
