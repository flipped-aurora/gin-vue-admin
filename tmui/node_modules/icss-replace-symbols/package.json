  ],
  "author": "Glen Maddern",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/css-modules/icss-replace-symbols/issues"
  },
  "homepage": "https://github.com/css-modules/icss-replace-symbols#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chokidar": "^1.3.0",
    "mocha": "^3.1.2",
    "postcss": "^6.0.1",
    "standard": "^8.4.0"
  }
